#include "litex_soc_linux.dtsi"

/ {
	model = "LiteX VexRiscV-SoC-Linux";
	compatible = "litex,vexriscv-soc-linux";

//memory_region,emulator_ram,0x20000000,16384, ???

	aliases {
		rom = &rom;
		sram = &sram;
	};

	/* ARTY board */
	rom: rom@00000000 {
		compatible = "mmio-sram";
		reg = <0x00000000 0x00008000>;
		read-only;
	};

	sram: sram@10000000 {
		compatible = "mmio-sram";
		reg = <0x20000000 0x00004000>;
	};

	main_ram: memory@c0000000 {
		device_type = "memory";
		reg = <0xc0000000 0x10000000>;
	};
};

&uart0 {
	status = "okay";
};

&mac0 {
	status = "okay";
};
